{
  "name": "fahrzeugfehler-de",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=2048 --max-semi-space-size=64 --expose-gc\" next dev",
    "dev:turbo": "cross-env NODE_OPTIONS=\"--max-old-space-size=2048 --max-semi-space-size=64\" next dev --turbopack",
    "dev:no-turbo": "cross-env NODE_OPTIONS=\"--max-old-space-size=1536 --max-semi-space-size=64\" next dev",
    "dev:memory": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096 --expose-gc --max-semi-space-size=128\" next dev",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "clean:cache": "rm -rf .next/cache",
    "clean:turbo": "rm -rf .next/cache/turbo && rm -rf node_modules/.cache/turbo",
    "prebuild": "tsx scripts/cleanupOldSitemaps.ts",
    "build": "next build",
    "sitemap:generate": "tsx scripts/generateSitemaps.ts",
    "sitemap:split": "tsx scripts/splitSitemapIndexes.ts",
    "sitemap:safeguard": "echo 'ðŸš« next-sitemap is disabled to prevent conflicts with our custom sitemap structure' && exit 0",
    "postbuild": "npm run sitemap:generate && npm run sitemap:split",
    "start": "next start",
    "lint": "next lint",
    "bulk-indexnow": "npx tsx scripts/bulkSubmitToIndexNow.ts",
    "verify-indexnow": "npx tsx scripts/verifyIndexNow.ts",
    "bulk-generate": "npx tsx scripts/bulkPageGeneration.ts",
    "bulk-generate:resume": "npx tsx scripts/bulkPageGeneration.ts --resume",
    "bulk-generate:limit": "npx tsx scripts/bulkPageGeneration.ts --limit=50",
    "clean:cache": "node scripts/clean-cache.js",
    "clean:cache:all": "node scripts/clean-cache.js --all",
    "clean:cache:win": "powershell -ExecutionPolicy Bypass -File scripts/clean-cache.ps1",
    "clean:cache:win:all": "powershell -ExecutionPolicy Bypass -File scripts/clean-cache.ps1 -All"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.821.0",
    "@langchain/openai": "^0.5.10",
    "@microsoft/clarity": "^1.0.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "canvas": "^3.2.0",
    "critters": "^0.0.23",
    "framer-motion": "^11.18.2",
    "geist": "^1.4.2",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.22",
    "lucide-react": "^0.511.0",
    "next": "15.5.7",
    "node-fetch": "^3.3.2",
    "openai": "^4.100.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-icons": "^5.5.0",
    "react-katex": "^3.1.0",
    "react-markdown": "^10.1.0",
    "rehype": "^13.0.2",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "sanitize-html": "^2.17.0",
    "three": "^0.181.2",
    "undici": "^7.9.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/sanitize-html": "^2.16.0",
    "cross-env": "^7.0.3",
    "eslint": "9.29.0",
    "eslint-config-next": "15.5.7",
    "next-sitemap": "^4.2.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
